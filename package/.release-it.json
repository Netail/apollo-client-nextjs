{
  "git": {
    "commitMessage": "chore: release v${version}",
    "requireCommits": true,
    "changelog": "git log --pretty=format:'* %s (%h)' -- ${from}...${to}  `git rev-parse --show-toplevel`/package",
    "requireCleanWorkingDir": false
  },
  "github": {
    "autoGenerate": true,
    "release": true,
    "releaseName": "v${version}"
  }
}
